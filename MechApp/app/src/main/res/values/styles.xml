<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="commonTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="commonTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style name="main_tab_ly_style">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="activityAnimation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/slide_right_in</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_left_in</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_right_out</item>
    </style>


    <style name="main_tab_tv_style">
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/main_tab_text_color</item>
        <item name="android:textSize">@dimen/main_tab_tv_size</item>
    </style>

    <style name="main_tab_tv_style_other" parent="main_tab_tv_style">
        <item name="android:layout_marginTop">20dp</item>
    </style>

    <style name="h_view_10">
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="h_view_1">
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="h_view_6">
        <item name="android:background">@color/item_tv_date</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="v_view_1">
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:layout_width">1dp</item>
    </style>


    <style name="tv_style_22_b5">
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@drawable/tv_color</item>
        <item name="gravity">center</item>
        <item name="android:drawableBottom">@drawable/tv_under_ine</item>
        <item name="android:drawablePadding">@dimen/dp_5</item>
    </style>

    <style name="user_home_page_tv">
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@drawable/tv_color_b_y</item>
        <item name="gravity">center</item>
        <item name="android:drawableBottom">@drawable/tv_under_ine</item>
        <item name="android:drawablePadding">@dimen/dp_5</item>
    </style>


    <style name="tv_style_22_b5_no_under">
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@drawable/tv_color</item>
        <item name="gravity">center</item>
    </style>
    <style name="tv_order">
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@drawable/tv_color_b_y</item>
        <item name="gravity">center</item>
    </style>
    
    <style name="tv_screen_style">
        <item name="android:layout_width">83dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/color_222</item>
    </style>

    <style name="recuit_tv_label_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:textSize">@dimen/sp_13</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:paddingLeft">@dimen/dp_8</item>
        <item name="android:paddingRight">@dimen/dp_8</item>
<!--        <item name="android:background">@drawable/rect_radio_2_g</item>-->
    </style>

    <style name="App_RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_start</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="distance_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableLeft">@mipmap/jl</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:textColor">#3A7EFF</item>
        <item name="android:text">距离205km</item>
        <item name="android:textSize">13sp</item>
    </style>


    <style name="ly_style_h">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="tv_style_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tv_style_left1" parent="tv_style_left">
        <item name="android:drawableLeft">@drawable/circle_red</item>
    </style>
    <style name="et_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textColorHint">#cacaca</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@null</item>
    </style>

    <style name="tv_style_right">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textColorHint">#cacaca</item>
    </style>

    <style name="btn_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_marginBottom">@dimen/dp_20</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:layout_marginRight">@dimen/dp_10</item>
        <item name="background">@drawable/rect_radio_6_y</item>
        <item name="gravity">center</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textSize">@dimen/sp_16</item>
    </style>
    <style name="tv_setting_style_left">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tv_setting_style_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textColor">@color/color_999</item>
        <item name="android:textColorHint">#cacaca</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@null</item>
    </style>



    <style name="tv_order_style_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textSize">@dimen/sp_13</item>

        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tv_order_style_right">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/sp_13</item>
        <item name="android:textColor">@color/color_222</item>
        <item name="android:textColorHint">#cacaca</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@null</item>
    </style>

</resources>